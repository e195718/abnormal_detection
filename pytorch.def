BootStrap: localimage
From: pytorch_latest.sif
# singularity pull docker://pytorch/pytorch:latest

%post
    apt-get update
    apt-get install -y libgl1-mesa-dev
    apt install -y --no-install-recommends graphviz
    conda install pytorch==1.10.1 
    conda install torchvision==0.11.2 
    conda install torchaudio==0.10.1 
    conda install cudatoolkit=11.3 -c pytorch -c conda-forge
